pipeline {
    agent any
      stages {
        stage('git clone') {
            steps {
                git branch: 'main', url: 'https://github.com/udayjy/lms.git'
            }
        }
        stage('code build') {
            steps {
                -bash 'cd webapp && npm install'
                -bash 'cd webapp && npm run build'
            }
        }
    }
}    
